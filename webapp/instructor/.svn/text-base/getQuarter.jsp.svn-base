<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<c:forEach items="${sites}" var="site">
<ul>
<li><a href="<c:url value='/sites/${site.section.quarter.quarterName}${site.section.quarter.year}/${site.section.course.code}/section${site.section.number}/' />">${site.section.course.code} ${site.section.course.name}, Section ${site.section.number}</a></li>
</ul>
</c:forEach>




